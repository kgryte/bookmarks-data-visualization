{
    "manifest_version": 2,

    "name": "Data Visualization Bookmarks",
    "version": "1.0",
    "description": "Collection of data visualization bookmarks",

    "homepage_url": "http://kgryte.com",

    "background": {
        "persistent": false,
        "scripts": ["js/jquery-1.10.2.min.js", "js/background.js"]
    },

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "Data Visualization Bookmarks"
    },
    "permissions": [
        "tabs"
    ],
    "web_accessible_resources": [
        "data/bookmarks.json"
    ]

}